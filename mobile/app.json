{
  "expo": {
    "name": "Storyteller",
    "slug": "storyteller",
    "scheme": "storyteller",
    "version": "0.2.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#030d25"
    },
    "ios": {
      "supportsTablet": false,
      "config": {
        "usesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.sstur.storyteller"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "extra": {
      "productionApiHost": "https://api.storyteller.fm",
      "eas": {
        "projectId": "9f192f55-0211-494a-8c62-fd261435ec11"
      }
    }
  }
}
